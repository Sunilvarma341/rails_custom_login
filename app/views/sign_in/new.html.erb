<h1>Sign In</h1>

<%= form_with url: sign_in_path do |form| %>
  <%= form.label :email %>
  <%= form.email_field :email %>

  <%= form.label :password %>
  <%= form.password_field :password %>

  <%= form.submit "Sign In" %>
<% end %>

<%= link_to "Forgot password?", reset_password_path %>
<br/>
<%= link_to "Sign up" ,  sign_up_path %>
